{
	"name": "orkan/wp-base1",
	"type": "wordpress-plugin",
	"description": "Wordpress plugin: WP Base Example 1",
	"homepage": "https://github.com/orkan/wp-base1",
	"license": "MIT",
	"authors": [
		{
			"name": "Orkan",
			"role": "Lead",
			"email": "orkans+new@gmail.com"
		}
	],
	"require": {
		"orkan/wp-base": "^1"
	},
	"autoload": {
		"psr-4": {
			"Orkan\\WP\\Base1\\": "src"
		}
	},
	"extra": {
		"installer-name": "ork-base1",
		"installer-paths": {
			"html/wp-content/plugins/{$name}": [
				"type:wordpress-plugin"
			],
			"html/wp-content/themes/{$name}": [
				"type:wordpress-theme"
			]
		},
		"branch-alias": {
			"dev-master": "1.0.x-dev",
			"dev-develop": "1.x-dev"
		}
	}
}
